{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": { "maxDuration": 30 }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "FOR4PAYMENTS_API_KEY": "6c1c43c0-c504-438d-bc39-ecbfd65c2f27",
    "UTMIFY_API_TOKEN": "JYGvNA3D1cBxNv6w6iLnk0p65FsI4kYXZyWq",
    "OPENAI_API_KEY": ""
  }
}